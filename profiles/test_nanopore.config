/*
    Test profile for AliNe

    Usage:
        nextflow run -profile docker,test aline.nf
*/
params {
    reads = "$baseDir/test/nanopore"
    genome = "$baseDir/test/nanopore/yeast.fa"
    single_end = false
    outdir = "results"
    aligner = 'bbmap'
    long_reads = true
    multiqc_config = "$baseDir/config/multiqc_conf.yml"
}